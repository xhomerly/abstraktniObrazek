<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="765.0" prefWidth="745.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.xhomerly.abstraktniobrazek.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Canvas fx:id="mainCanvas" height="554.0" width="516.0" />
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#delete" text="Smazat obrázek" />
            <TextField fx:id="textField" alignment="CENTER" prefHeight="25.0" prefWidth="41.0" text="1" />
            <Button mnemonicParsing="false" onAction="#create" text="Nakresli obrázek" />
            <Button mnemonicParsing="false" onAction="#graphX" text="Nakresli graf rozložení X" />
            <Button mnemonicParsing="false" onAction="#graphY" text="Nakresli graf rozložení Y" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </HBox>
   </children>
</VBox>
